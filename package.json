{
    "name": "humanid-core",
    "version": "1.0.0",
    "description": "humanID API Core",
    "main": "src/index.js",
    "engines": {
        "node": "16.x"
    },
    "scripts": {
        "start": "node src/index.js",
        "db:up": "npx sequelize db:migrate",
        "db:down": "npx sequelize db:migrate:undo",
        "db:status": "sequelize db:migrate:status",
        "db:seed:fixAppConfig": "sequelize db:seed --seed 20220529064148-fix-app-config.js",
        "gen-aes-key": "node scripts/generate-aes-key.js"
    },
    "keywords": [
        "human",
        "id"
    ],
    "author": "yohanes.gultom@gmail.com",
    "license": "GPL-3.0",
    "dependencies": {
        "@sentry/cli": "^2.30.2",
        "@sentry/node": "^7.108.0",
        "@sentry/profiling-node": "^7.108.0",
        "argon2": "^0.28.5",
        "aws-sdk": "^2.1098.0",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.2",
        "cors": "^2.8.5",
        "cron": "^3.1.6",
        "dotenv": "^16.0.0",
        "express": "^4.17.3",
        "joi": "^17.6.0",
        "jsonwebtoken": "^9.0.0",
        "libphonenumber-js": "^1.10.14",
        "lodash": "^4.17.21",
        "multer": "^1.4.5-lts.1",
        "mysql2": "^2.3.3",
        "nanoid": "^3.3.1",
        "node-fetch": "^2.6.7",
        "nodemailer": "^6.7.3",
        "sequelize": "^6.17.0",
        "sequelize-cli": "^6.4.1",
        "winston": "^3.6.0"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/express": "^4.17.13",
        "@types/node": "^13.13.52",
        "@types/node-fetch": "^2.6.1",
        "@types/sequelize": "^4.28.11",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^3.4.1",
        "prettier": "^2.6.0",
        "sequelize-auto": "^0.8.8"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:human-internet/humanid-core.git"
    }
